import synchro_pb2
import socket
import mysql.connector
import sys

import testProto

protobufProccess = testProto.ProtobufProcessing("Car", "localhost", "root", "root", "fcity_client")

HOST, PORT = "localhost", 172.31.3.59
data = synchro_pb2.CarToServ()
data.connectionRequest.Clear()
# Create a socket (SOCK_STREAM means a TCP socket)
sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)

try:
    # Connect to server and send data
    sock.connect((HOST, PORT))
    sock.sendall(data.SerializeToString())

    # Receive data from the server and shut down
    recv = sock.recv(1024)
 
    msg = synchro_pb2.ServToCar.FromString(recv)
finally:
    sock.close()

print("Sent:     {}".format(data.SerializeToString()))
print("Received: {}".format(msg.connectionResponse.port))

HOST, PORT = "localhost", msg.connectionResponse.port
data = synchro_pb2.CarToServ()
data.synchronizeRequest.Clear()
# Create a socket (SOCK_STREAM means a TCP socket)
sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)

try:
    # Connect to server and send data
    sock.connect((HOST, PORT))
    sock.sendall(data.SerializeToString())

    # Receive data from the server and shut down
    recv = sock.recv(10024)

    protobufProccess.protobufDataToDb(recv);
 

finally:
    sock.close()

print("Sent:     {}".format(data.SerializeToString()))
print("Received: {}".format(recv))
